{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "exportall.config.namedExports": true,
  "exportall.config.exclude": ["-test.", ".test.", ".spec."],
  "exportall.config.semis": false,
  "exportall.config.folderListener": ["./app/core/**", "./app/features/**"],
  "exportall.config.quote": "\"",
  "exportall.config.relExclusion": ["/app/shared/helpers"]
}
